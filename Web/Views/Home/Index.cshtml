            <div class="section-title col-sm-12">
                <h3>نمونه کارها اخیر</h3>
            </div>
            <div class="part col-sm-12">
                <div class="portfolio-nav col-sm-12" id="filter-button">
                    <ul>
                        <li data-filter="*" class="current"> <span>همه دسته بندی ها</span></li>
                        <li data-filter=".user-interface"><span>طراحی وب سایت</span></li>
                        <li data-filter=".branding"><span>علائم تجاری</span></li>
                        <li data-filter=".mockup"><span>ماکت ها</span></li>
                        <li data-filter=".ui"><span>ماکت ها</span></li>
                    </ul>
                </div>
                <div class="mh-project-gallery col-sm-12" id="project-gallery">
                    <div class="portfolioContainer row">
                        @if (projects.Count > 0)
                        {
                            @foreach (var item in projects)
                            {
                                <div class="grid-item col-md-4 col-sm-6 col-xs-12 user-interface">
                                    <figure>
                                        <img src="/images/projects/@item.ImageName" alt="@item.Title" width="340" height="246">
