                                            <i class="fa fa-search"></i>
                                            <h5 class="title">@item.Title</h5>
                                            <span class="sub-title">عکاسی</span>
                                            <a data-fancybox data-src="#mh"></a>
                                        </figcaption>
                                    </figure>
                                </div>
                            }
                        }
